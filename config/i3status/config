general {
        colors = false
        interval = 5
}

order += "wireless _first_"
order += "volume master"
order += "tztime local"

wireless _first_ {
        format_up = " %quality "
        format_down = " Down "
}

volume master {
	format = "  %volume "
        format_muted = "  %volume "
        device = "default"
        mixer = "Master"
        mixer_idx = 0
}

tztime local {
        format = "%a %d-%m-%Y %H:%M "
}
